	<queryString language="SQL">
		<![CDATA[SELECT
        ROW_NUMBER()
        OVER(
            ORDER BY
                super.idper DESC
        )AS fila,
        super.idper,
        super.nomper,
        super.apeper,
        super.pasper,
        super.emaper,
        concat(concat(ubigeo.DISUBG,' '), concat(concat(ubigeo.PROUBG,' '),  ubigeo.DEPUBG)) AS UBIGEO,
        super.dniper,
        super.celper,
        super.rolper,
        super.estper,
        concat(concat (infer.nomper,' '), infer.apeper) AS relacion
    FROM
        persona super
        LEFT JOIN persona infer ON super.persona_idper = infer.idper
        INNER JOIN ubigeo ON super.idubg = ubigeo.idubg
        where super.rolper='ALUMNO']]>
	</queryString>
